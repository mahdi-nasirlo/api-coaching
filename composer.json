{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ariaieboy/filament-jalali-datetime": "^1.2",
        "ariaieboy/filament-jalali-datetimepicker": "^2.0",
        "bezhansalleh/filament-language-switch": "^2.0",
        "bezhansalleh/filament-panel-switch": "^0.0.3",
        "coolsam/modules": "^3.0@beta",
        "cviebrock/eloquent-sluggable": "^10.0",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0",
        "filament/spatie-laravel-settings-plugin": "^3.0",
        "filament/spatie-laravel-tags-plugin": "^3.0",
        "filament/spatie-laravel-translatable-plugin": "^3.0-stable",
        "flowframe/laravel-trend": "^0.1.1",
        "fruitcake/laravel-cors": "dev-develop",
        "guzzlehttp/guzzle": "^7.2",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.15",
        "laravel/tinker": "^2.8",
        "mokhosh/filament-jalali": "^3.1",
        "nwidart/laravel-modules": "^10.0",
        "shetabit/payment": "^5.4",
        "squirephp/countries-en": "^3.3",
        "squirephp/currencies-en": "^3.3",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.1",
        "phpunit/phpunit": "^10.1",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
